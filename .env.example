NODE_ENV=development
PORT=4000
# Postgres connection string (local example)
# postgresql://<user>:<password>@localhost:5432/loanshark?schema=public
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/loanshark?schema=public

# Admin API key required for admin GraphQL requests and REST login responses
API_KEY=change-me

# Admin credentials for REST login endpoint (/api/admin/login)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=changeme